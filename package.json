{
	"name": "infilib",
	"version": "0.4.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write  --plugin prettier-plugin-svelte .",
		"postinstall": "npx prisma db push --force-reset --accept-data-loss && npx prisma generate --no-engine && npx prisma db seed"
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.3.1",
		"@prisma/client": "^5.8.1",
		"@sveltejs/adapter-vercel": "^3.1.0",
		"@sveltejs/kit": "^1.20.4",
		"html5-qrcode": "^2.3.8",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"prisma": "^5.8.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-select": "^5.7.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@passageidentity/passage-elements": "^1.16.0",
		"@passageidentity/passage-node": "^2.6.0",
		"@prisma/extension-accelerate": "^0.6.2",
		"@vercel/analytics": "^1.1.1",
		"@vercel/speed-insights": "^1.0.2",
		"csv-parser": "^3.0.0",
		"dayjs": "^1.11.10",
		"flatpickr": "^4.6.13",
		"jose": "^5.1.3",
		"papaparse": "^5.4.1"
	}
}
